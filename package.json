{
  "name": "bot-whatsapp-deputado",
  "version": "1.0.0",
  "description": "Bot WhatsApp para Deputado com Next.js e PostgreSQL",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:bot\"",
    "dev:next": "next dev -p 3003",
    "dev:bot": "node bot/index.js",
    "start:bot": "node scripts/start-bot.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:migrate:sqlite": "node scripts/migrate-sqlite.js",
    "db:seed:sqlite": "node scripts/seed-sqlite.js",
    "setup:dev": "npm run db:migrate:sqlite && npm run db:seed:sqlite",
    "sync:camara": "node scripts/sync-camara.js",
    "test:scheduler": "node test-scheduler.js",
    "scheduler:start": "node -e \"import('./src/lib/scheduler.js').then(m => m.camaraScheduler.startScheduler())\"",
    "scheduler:stop": "node -e \"import('./src/lib/scheduler.js').then(m => m.camaraScheduler.stopScheduler())\""
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "whatsapp-web.js": "^1.23.0",
    "qrcode-terminal": "^0.12.0",
    "fuse.js": "^7.0.0",
    "libphonenumber-js": "^1.11.5",
    "dayjs": "^1.11.12",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "concurrently": "^8.2.2",
    "better-sqlite3": "^9.4.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.7",
    "typescript": "^5.2.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31"
  }
}
